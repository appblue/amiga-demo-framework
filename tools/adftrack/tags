AddFile	adftrack.c	/^void AddFile( FileEntry& fileentry, unsigned char* buf, int& fileoffset ) {$/;"	f
BSwapInt	adftrack.c	/^uint32_t BSwapInt( uint32_t value ) {$/;"	f
BSwapIntArray	adftrack.c	/^void BSwapIntArray( unsigned int* values, int size ) {$/;"	f
FileEntry	adftrack.c	/^	FileEntry( char* filename ) {$/;"	f	struct:FileEntry
FileEntry	adftrack.c	/^	FileEntry() {$/;"	f	struct:FileEntry
FileEntry	adftrack.c	/^struct FileEntry {$/;"	s	file:
PatchBootBlockCheckSum	adftrack.c	/^void PatchBootBlockCheckSum( unsigned char* buf ) {$/;"	f
ULONG_MAX_32	adftrack.c	/^#define ULONG_MAX_32 /;"	d	file:
filename	adftrack.c	/^	char*	filename;		\/\/ the filename$/;"	m	struct:FileEntry	file:
main	adftrack.c	/^int main(int argc, char **argv) {$/;"	f
numblocks	adftrack.c	/^	int		numblocks;		\/\/ the amount of blocks\/sectors the file occupies on disk.$/;"	m	struct:FileEntry	file:
startblock	adftrack.c	/^	int		startblock;     \/\/ the starting offset on disk in blocks$/;"	m	struct:FileEntry	file:
startoffset	adftrack.c	/^	int		startoffset;	\/\/ the starting offset on disk in bytes$/;"	m	struct:FileEntry	file:
std	adftrack.c	/^using namespace std;$/;"	v
